{"appenders": [
	{"type":"console"},
	{
		"type":"dateFile",
		"filename":"logs/http/",
		"pattern":"yyyy-MM-dd-hh",
		"alwaysIncludePattern": true,
		"level":"info",
		"category":"app"
	},
	{
		"type":"file",
		"filename":"logs/err/err",  
		"maxLogSize": 20480,  
		"backups": 3,
		"level":"info",
		"category":"log"
	}
]}